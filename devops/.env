cat > devops/.env.example <<'EOF'
# devops/.env.example - fill devops/.env locally from this template (DO NOT COMMIT devops/.env)

# AWS region to deploy into
AWS_REGION=ap-south-1
     

# Cognito domain prefix (must be unique per region)
DOMAIN_PREFIX=fastapi-auth-yourname

# SES verified identity ARN (example placeholder) - MUST be verified in the same region (ap-south-1)
EMAIL_IDENTITY_ARN=arn:aws:ses:<region>:<account-id>:identity/noreply@yourproject.com

# Callback and logout urls (comma-separated)
CALLBACK_URLS=http://localhost:8000/api/v1/auth/callback
LOGOUT_URLS=http://localhost:3000/logout

# Optional: Google OAuth credentials (leave empty to skip)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Optional: Facebook OAuth credentials (leave empty to skip)
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=
EOF

# Stage the example
git add devops/.env.example
git commit -m "Update devops/.env.example: set region placeholder and SES ARN placeholder"